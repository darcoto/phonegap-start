	id		= "com.darcoto.started"
	version 	= "1.0.1">

	<name>PhoneGap: Darco start</name>

	<description>
		My first phonegap
	</description>

	<author href="http://darco.github.com"
		email="veselin@darcoto.net">
		Veselin Malezanov
	</author>

	<icon src="icon.png" gap:role="default" />

	<feature name="http://api.phonegap.com/1.0/geolocation"/>
